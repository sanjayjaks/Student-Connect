        </nav>
    </header>
    <main>
        <div class="main-content">
            <h1>Ongoing and Completed Meetings</h1>
            <div class="meeting-links">
                <% if (links.length > 0) { %>
                    <ul>
                        <% links.forEach(link => { %>
                            <li><a href="<%= link.name %>" target="_blank"><%= link.name %></a></li>
                        <% }) %>
                    </ul>
                <% } else { %>
                    <p>No meetings available.</p>
                <% } %>
            </div>
        </div>
    </main> 
